@model IEnumerable<Web1.Models.JoinTable>
@{
    Layout = "~/Views/Shared/_LayoutPatient.cshtml";
}
<main>

    <div class="slider-area2">
        <div class="slider-height2 d-flex align-items-center">
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="hero-cap hero-cap2 text-center">
                            <h2>My previouse appointment </h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="team-area section-padding30">
        <div class="container">

            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-xl-4 col-lg-4 col-md-6 col-sm-">
                        <div class="single-team mb-30">
                            <div class="team-img">
                            </div>
                            <div class="team-caption">
                                <h3>@item.JTclinic.ClinicName</h3>
                                <h3>@item.JTuser.FullName</h3>
                                <h3>@item.JTavailabiltyDate.Date?.ToString("dd/MM/yyy")</h3>
                                <h3>@item.JTappontment.Medesine</h3>
                                <h3>@item.JTappontment.MidCase</h3>                              
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    <!-- Team End -->
</main>